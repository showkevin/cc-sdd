{
  "version": 1,
  "artifacts": [
    {
      "id": "commands_kiro_os_mac",
      "source": {
        "type": "templateDir",
        "fromDir": "templates/agents/{{AGENT}}/commands/os-mac/kiro",
        "toDir": ".claude/commands/kiro"
      },
      "when": { "agent": "claude-code", "os": ["mac"] }
    },
    {
      "id": "commands_kiro_rapid_os_mac",
      "source": {
        "type": "templateDir",
        "fromDir": "templates/agents/{{AGENT}}/commands/os-mac/kiro-rapid",
        "toDir": ".claude/commands/kiro-rapid"
      },
      "when": { "agent": "claude-code", "os": ["mac"] }
    },
    {
      "id": "commands_kiro_os_windows",
      "source": {
        "type": "templateDir",
        "fromDir": "templates/agents/{{AGENT}}/commands/os-windows/kiro",
        "toDir": ".claude/commands/kiro"
      },
      "when": { "agent": "claude-code", "os": ["windows", "linux"] }
    },
    {
      "id": "commands_kiro_rapid_os_windows",
      "source": {
        "type": "templateDir",
        "fromDir": "templates/agents/{{AGENT}}/commands/os-windows/kiro-rapid",
        "toDir": ".claude/commands/kiro-rapid"
      },
      "when": { "agent": "claude-code", "os": ["windows", "linux"] }
    },
    {
      "id": "doc_main",
      "source": {
        "type": "templateFile",
        "from": "templates/agents/{{AGENT}}/docs/CLAUDE.tpl.md",
        "toDir": ".",
        "rename": "{{AGENT_DOC}}"
      },
      "when": { "agent": "claude-code" }
    }
  ]
}
